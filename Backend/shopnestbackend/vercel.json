
{
    "version": 2,
    "builds": [
        {
            "src": "./routes/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "destination": "./routes/index.js"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }] 
    
}